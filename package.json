{
  "name": "happypandax-client",
  "version": "3.1.0",
  "description": "A javascript client library for communicating with HappyPanda X servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "concurrently -i --default-input-target 1 \"python test/timeout.py\" \"jest\"",
