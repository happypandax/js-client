    "docs": "jsdoc2md --configure ./jsdoc2md.json --heading-depth 4 ./src/index.ts",
    "prepare": "yarn build"
  },
  "repository": "git+https://github.com/happypandax/js-client.git",
  "keywords": [
    "hpx",
    "happypandax",
    "client",
    "socket",
